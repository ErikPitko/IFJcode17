' nested loops

scope

dim a as integer = 0
dim b as integer = 0
dim c as integer = 0
	
	do while a < 4
		
		print a;

		do while b < 4
		
			print b;
 	
			b = b + 1
		loop

		a = a + 1
	loop

	a = 0	

	do while a < 4
		
		print a;

		do while c < 8
		
			print c;
 	
			c = c + 1
		loop

		a = a + 1
	loop	
	print !"\n";
end scope
