' if then elseif else killer
scope

	dim a as integer = 1
	
	if a > 2 then

		if a < 2 then
			print 2;
		elseif a < 2 then 
			print 3;
		else 
			print 4;
		end if

	elseif a > 2 then

		if a < 2 then
			print 7;
		elseif a < 2 then 
			print 8;
		else 
			print 9;
		end if

	else

		if a > 2 then
			print 12;
		elseif a > 2 then 
			print 13;
		else 
			print 14;
		end if

	end if
	print !"\n";
end scope
