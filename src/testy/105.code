' loop from one to 1024
scope

	dim i as integer = 0
	dim j as integer = 10
	dim vypis as integer

	do while i < 1024

		if i >= 1000 then
            do while j <> 100					
			    vypis = vypis + j

			    j = j + 1
		       loop
        end if

		i = i + 1
	loop

	print vypis;
	print !"\n";

end scope
